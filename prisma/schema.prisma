generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id          Int      @id @default(autoincrement())
  title       String
  description String
  image       String?
  url         String?
  createdAt   DateTime @default(now())
}

model Message {
  id        Int      @id @default(autoincrement())
  name      String?
  email     String?
  message   String
  createdAt DateTime @default(now())
}
